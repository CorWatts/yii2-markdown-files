{
  "name": "corwatts/yii2-markdown-files",
  "description": "A simple flat-file Markdown renderer",
  "keywords": [
    "yii2",
    "blog",
    "flat",
    "file",
    "markdown"
  ],
  "homepage": "https://github.com/CorWatts/yii2-markdown-files",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Corey Watts",
      "email": "corey@haaswatts.com"
    }
  ],
  "support": {
    "issues": "https://github.com/CorWatts/yii2-markdown-files/issues",
    "source": "https://github.com/CorWatts/yii2-markdown-files"
  },
  "scripts": {
    "test": "codecept run",
    "test-scaffold": "codecept build",
    "test-coverage": "XDEBUG_MODE=coverage codecept run --coverage --coverage-xml --coverage-html --ansi"
  },
  "require": {
    "hyn/frontmatter": "dev-master",
    "yiisoft/yii2": "~2.0.0"
  },
  "require-dev": {
    "codeception/verify": "^3.0",
    "codeception/module-asserts": "^3.0",
    "codeception/module-yii2": "^1.1",
    "codeception/codeception": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "corwatts\\MarkdownFiles\\": ""
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
