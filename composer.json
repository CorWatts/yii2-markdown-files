{
  "name": "corwatts/yii2-markdown-files",
  "description": "A simple flat-file Markdown renderer",
  "keywords": [
    "yii2",
    "blog",
    "flat",
    "file",
    "markdown"
  ],
  "homepage": "https://github.com/CorWatts/yii2-markdown-files",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Corey Watts",
      "email": "corey@haaswatts.com"
    }
  ],
  "support": {
    "issues": "https://github.com/CorWatts/yii2-markdown-files/issues",
    "source": "https://github.com/CorWatts/yii2-markdown-files"
  },
  "scripts": {
    "test": "codecept run",
    "test-scaffold": "codecept build",
    "test-coverage": "codecept run --coverage --coverage-xml --coverage-html --ansi"
  },
  "require": {
    "php": ">=7.0",
    "hyn/frontmatter": "~1.1",
    "yiisoft/yii2": "*"
  },
  "require-dev": {
    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1",
    "codeception/specify": "~0.4.3",
    "codeception/codeception": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "corwatts\\MarkdownFiles\\": ""
    }
  }
}
